    "ejs-compiled-loader": "^1.1.0",
    "ejs-html-loader": "^4.0.1",
    "ejs-loader": "^0.3.3",
    "extract-loader": "^3.1.0",
    "extricate-loader": "^3.0.0",
    "file-loader": "^3.0.1",
    "generate-package-json-webpack-plugin": "^1.0.0",
    "glob": "^7.1.3",
    "html-beautify-loader": "0.0.1",
    "html-loader": "^0.5.5",
